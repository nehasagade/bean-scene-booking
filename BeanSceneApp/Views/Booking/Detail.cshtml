@model BeanSceneApp.Models.Booking
@{
    ViewData["Title"] = "Booking Detail";
}

<h1>Detail</h1>
<div class="row">
    <div class="col-md-6    ">  
        <div class="form-group">
            <label asp-for="@Model.FirstName" class="control-label"></label>
            <input asp-for="@Model.FirstName" class="form-control" disabled/>
        </div>
        <div class="form-group">
            <label asp-for="@Model.LastName" class="control-label"></label>
            <input asp-for="@Model.LastName" class="form-control" disabled/>
        </div>
        <div class="form-group">
            <label asp-for="@Model.Phone" class="control-label"></label>
            <input asp-for="@Model.Phone" class="form-control" disabled />
        </div>
        <div class="form-group">
            <label asp-for="@Model.Email" class="control-label"></label>
            <input asp-for="@Model.Email" class="form-control" disabled />
        </div>
        <div class="form-group">
            <label asp-for="@Model.Date" class="control-label"></label>
            <input asp-for="@Model.Date"  class="form-control" disabled />
        </div>
        <div class="form-group">
            <label asp-for="@Model.StartTime" class="control-label"></label>
            <input asp-for="@Model.StartTime" class="form-control" disabled />
        </div>
        <div class="form-group">
            <label asp-for="@Model.NumOfGuests" class="control-label"></label>
            <input asp-for="@Model.NumOfGuests" class="form-control" disabled />
        </div>
        <div class="form-group">
            <label asp-for="@Model.Note" class="control-label"></label>
            <input asp-for="@Model.Note" class="form-control" disabled />
        </div>
        <div class="form-group">
            <label asp-for="@Model.Status" class="control-label"></label>
            <input asp-for="@Model.Status" class="form-control" disabled />
        </div>
        <div class="form-group">
            <label asp-for="@Model.Source" class="control-label"></label>
            <input asp-for="@Model.Source" class="form-control" disabled />
        </div>
        
    </div>
</div>

<div class="my-1">
    <a type="button" class="btn btn-primary" href="@Url.Action("Index")">
       <span>Back</span>
    </a>
</div>
